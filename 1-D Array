#include<iostream>
using namespace std;

int main()
{

  int array[6]={1,2,4,6,7,8}; //Declaration of array of type int having index of size 5;
  //To print an array we can print each element through index 
   //Printing array using index value one by one
   
cout<<"Array[0] = "<<array[0]<<endl;
cout<<"Array[1] = "<<array[1]<<endl;
cout<<"Array[2] = "<<array[2]<<endl;
cout<<"Array[3] = "<<array[3]<<endl;
cout<<"Array[4] = "<<array[4]<<endl;
cout<<"Array[5] = "<<array[5]<<endl;


for(int i=0;<=5;i++)
   {

    	cout<<"Array["<<i<<"]=  "<<array[i]<<endl;
    }
return 0;
}
